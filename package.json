{
  "name": "fauxpilot",
  "displayName": "fauxpilot",
  "description": "",
  "version": "0.0.2",
  "repository": {
    "url": "https://github.com/Venthe/vscode-fauxpilot"
  },
  "license": "MIT",
  "publisher": "Venthe",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fauxpilot.enable",
        "title": "Enable fauxpilot"
      },
      {
        "command": "fauxpilot.disable",
        "title": "Disable fauxpilot"
      }
    ],
    "configuration": {
      "title": "Fauxpilot",
      "properties": {
        "fauxpilot.server": {
          "type": "string",
          "default": "http://localhost:5000/v1/engines"
        },
        "fauxpilot.engine": {
          "type": "string",
          "default": "codegen"
        },
        "fauxpilot.maxTokens": {
          "type": "number",
          "default": 4
        },
        "fauxpilot.maxLines": {
          "type": "number",
          "default": 150
        },
        "fauxpilot.suggestionDelay": {
          "type": "number",
          "default": 150
        },
        "fauxpilot.enabled": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "openai": "^3.0.0"
  }
}